<p id="notice"><%= notice %></p>

<h1>Parkings</h1>

 <div class="jumbotron jumbotron-fluid">

  <div class="container">
   <h1 class="display-4">Listado de estacionamientos</h1>
                <div class="row">
            <div class="col">
          <table class="table table-striped">
            <thead>
    <tr>
      <th>Id usuario</th>
      <th>Id ubicacion</th>
      <th>Id imagen</th>
      <th>Nombre</th>
      <th>Direccion</th>
      <th>Precio</th>
      <th>Telefono</th>
      <th>Id tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>

    <% @parkings.each do |parking| %>
    
      <tr>
        
        <td><%= parking.id_usuario %></td>
        <td><%= parking.id_ubicacion %></td>
        <td><%= parking.id_imagen %></td>
        <td><%= parking.nombre %></td>
        <td><%= parking.direccion %></td>
        <td><%= parking.precio %></td>
        <td><%= parking.telefono %></td>
        <td><%= parking.id_tipo %></td>
        <td><%= link_to 'Mostrar', parking, class: "btn btn-warning" %></td>
        <td><%= link_to 'Editar', edit_parking_path(parking), class: "btn btn-warning" %></td>
        <td><%= link_to 'Eliminar', parking, class: "btn btn-danger", method: :delete, data: { confirm: 'Estas seguro?' } %></td>
      </tr>

    <% end %>

  </tbody>

</table>
<%= link_to 'Crear nuevo', new_parking_path, class: "btn btn-info" %>
</div>
</div>

<br>
</div>

   </div>