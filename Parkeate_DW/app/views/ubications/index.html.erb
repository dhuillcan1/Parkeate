<p id="notice"><%= notice %></p>

<h1>Ubications</h1>

<table>
  <thead>
    <tr>
      <th>Id estacionamiento</th>
      <th>Cod departamento</th>
      <th>Cod provincia</th>
      <th>Cod distrito</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ubications.each do |ubication| %>
      <tr>
        <td><%= ubication.id_estacionamiento %></td>
        <td><%= ubication.cod_departamento %></td>
        <td><%= ubication.cod_provincia %></td>
        <td><%= ubication.cod_distrito %></td>
        <td><%= link_to 'Show', ubication %></td>
        <td><%= link_to 'Edit', edit_ubication_path(ubication) %></td>
        <td><%= link_to 'Destroy', ubication, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ubication', new_ubication_path %>
