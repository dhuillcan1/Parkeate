<%= form_with(model: rent, local: true) do |form| %>
  <% if rent.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(rent.errors.count, "error") %> prohibited this rent from being saved:</h2>

      <ul>
      <% rent.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :parking_id %>
    <%= form.collection_select :parking_id, Parking.all, :id, :nombre %>
  </div>

  <div class="field">
    <%= form.label :id_usuario %>
    <%= form.number_field :id_usuario, id: :rent_id_usuario %>
  </div>

  <div class="field">
    <%= form.label :fecha_inicio %>
    <%= form.datetime_select :fecha_inicio, id: :rent_fecha_inicio %>
  </div>

  <div class="field">
    <%= form.label :fecha_fin %>
    <%= form.datetime_select :fecha_fin, id: :rent_fecha_fin %>
  </div>

  <div class="field">
    <%= form.label :id_servicio_adicional %>
    <%= form.number_field :id_servicio_adicional, id: :rent_id_servicio_adicional %>
  </div>

  <div class="field">
    <%= form.label :observaciones %>
    <%= form.text_area :observaciones, id: :rent_observaciones %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
