<p id="notice"><%= notice %></p>

<h1>Rents</h1>

<table>
  <thead>
    <tr>
      <th>Id estacionamiento</th>
      <th>Id usuario</th>
      <th>Fecha inicio</th>
      <th>Fecha fin</th>
      <th>Id servicio adicional</th>
      <th>Observaciones</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @rents.each do |rent| %>
      <tr>
        <td><%= rent.id_estacionamiento %></td>
        <td><%= rent.id_usuario %></td>
        <td><%= rent.fecha_inicio %></td>
        <td><%= rent.fecha_fin %></td>
        <td><%= rent.id_servicio_adicional %></td>
        <td><%= rent.observaciones %></td>
        <td><%= link_to 'Show', rent %></td>
        <td><%= link_to 'Edit', edit_rent_path(rent) %></td>
        <td><%= link_to 'Destroy', rent, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Rent', new_rent_path %>
